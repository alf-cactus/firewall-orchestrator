[Unit]
Description=FW Orch Webhook receiver for github
After=network.target remote-fs.target nss-lookup.target
[Service]
WorkingDirectory={{ fworchtest_home }}
ExecStart=/usr/bin/python3 {{ fworchtest_home }}/fworch-webhook-receiver.py
StandardOutput=journal
StandardError=journal
SyslogIdentifier=fworch-webhook
User=fworchtest
Environment=
[Install]
WantedBy=multi-user.target
